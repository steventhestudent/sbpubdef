{
  "name": "sbpubdef-sol",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0 < 23.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "gulp bundle",
    "clean": "gulp clean",
    "test": "gulp test",
    "serve": "fast-serve",
    "tailwind:build": "pnpm npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./dist/tailwind.css --minify",
    "tailwind:watch": "pnpm npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./dist/tailwind.css --watch",
    "dev": "concurrently \"pnpm run tailwind:watch\" \"pnpm exec fast-serve\"",
    "theme": "concurrently \"pnpm run tailwind:watch\" \"pnpm exec fast-serve --config=themeInjector\"",
    "make": "pnpm npx gulp bundle --ship && pnpm npx gulp package-solution --ship"
  },
  "dependencies": {
    "@fluentui/react": "^8.106.4",
    "@microsoft/decorators": "1.21.1",
