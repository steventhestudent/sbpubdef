		"@pnp/spfx-property-controls": "^3.22.0",
		"@tailwindcss/postcss": "^4.1.14",
		"gulp-cli": "^3.1.0",
		"gulp-postcss": "^10.0.0",
		"mem-fs": "^4.1.2",
		"postcss": "^8.5.6",
		"powerbi-client": "^2.23.9",
		"react": "17.0.1",
		"react-dom": "17.0.1",
