    "@microsoft/sp-webpart-base": "1.21.1",
    "@tailwindcss/cli": "^4.1.15",
    "@tailwindcss/postcss": "^4.1.14",
    "gulp-cli": "^3.1.0",
    "gulp-postcss": "^10.0.0",
    "mem-fs": "^4.1.2",
    "postcss": "^8.5.6",
