{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "landing-redirect-ext-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/landingRedirectExt/LandingRedirectExtApplicationCustomizer.js",
          "manifest": "./src/extensions/landingRedirectExt/LandingRedirectExtApplicationCustomizer.manifest.json"
        }
      ]
    },
    "welcome-message-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcomeMessage/WelcomeMessageWebPart.js",
          "manifest": "./src/webparts/welcomeMessage/WelcomeMessageWebPart.manifest.json"
        }
      ]
    },
    "office-information-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/officeInformation/OfficeInformationWebPart.js",
          "manifest": "./src/webparts/officeInformation/OfficeInformationWebPart.manifest.json"
        }
      ]
    },
    "manuals-and-handbooks-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/manualsAndHandbooks/ManualsAndHandbooksWebPart.js",
          "manifest": "./src/webparts/manualsAndHandbooks/ManualsAndHandbooksWebPart.manifest.json"
        }
      ]
    },
    "most-common-forms-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mostCommonForms/MostCommonFormsWebPart.js",
          "manifest": "./src/webparts/mostCommonForms/MostCommonFormsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "LandingRedirectExtApplicationCustomizerStrings": "lib/extensions/landingRedirectExt/loc/{locale}.js",
    "WelcomeMessageWebPartStrings": "lib/webparts/welcomeMessage/loc/{locale}.js",
    "OfficeInformationWebPartStrings": "lib/webparts/officeInformation/loc/{locale}.js",
    "ManualsAndHandbooksWebPartStrings": "lib/webparts/manualsAndHandbooks/loc/{locale}.js",
    "MostCommonFormsWebPartStrings": "lib/webparts/mostCommonForms/loc/{locale}.js"
  }
}
